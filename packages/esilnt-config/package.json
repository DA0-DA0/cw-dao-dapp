{
  "name": "@project/eslint-config",
  "version": "0.0.0",
  "dependencies": {
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "eslintConfig": {
    "extends": [
      "./overrides.js",
      "plugin:prettier/recommended"
